{
  "name": "elm-review-example",
  "version": "1.0.0",
  "description": "Example for elm-review",
  "scripts": {
    "test": "elm make src/Main.elm && npm run elm-test --silent && npm run review --silent",
    "elm-test": "(cd review/ && elm-test)",
    "review": "elm-review",
    "review:fix": "npm run review --silent -- --fix"
  },
