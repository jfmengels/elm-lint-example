    "type": "git",
    "url": "git+https://github.com/jfmengels/elm-review-example.git"
  },
  "keywords": [
    "elm-review",
    "example"
  ],
  "author": "Jeroen Engels",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jfmengels/elm-review-example/issues"
  },
  "homepage": "https://github.com/jfmengels/elm-review-example#readme",
  "dependencies": {
    "elm": "^0.19.1-3"
  },
  "devDependencies": {
    "elm-review": "^2.0.0",
    "elm-test": "^0.19.1-revision2"
  }
}
